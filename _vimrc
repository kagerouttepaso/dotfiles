"vunlde.vimで管理してるpluginを読み込む
source ~/.vim/config/bundle.vimrc

"基本設定
source ~/.vim/config/basic.vimrc
"StatusLine設定
source ~/.vim/config/statusline.vimrc
"インデント設定
source ~/.vim/config/indent.vimrc
"表示関連
source ~/.vim/config/apperance.vimrc
"補完関連
source ~/.vim/config/completion.vimrc
"Tags関連
source ~/.vim/config/tags.vimrc
"検索関連
source ~/.vim/config/search.vimrc
"移動関連
source ~/.vim/config/moving.vimrc
"Color関連
source ~/.vim/config/colors.vimrc
"編集関連
source ~/.vim/config/editing.vimrc
"エンコーディング関連
source ~/.vim/config/encoding.vimrc
"その他
source ~/.vim/config/misc.vimrc
"プラグインに依存するアレ
source ~/.vim/config/plugins.vimrc
"tab周りの設定
source ~/.vim/config/tab.vimrc
