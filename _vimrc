
"vunlde.vimで管理してるpluginを読み込む
source ~/.vim/config/.vimrc.bundle

"基本設定
source ~/.vim/config/.vimrc.basic
"StatusLine設定
source ~/.vim/config/.vimrc.statusline
"インデント設定
source ~/.vim/config/.vimrc.indent
"表示関連
source ~/.vim/config/.vimrc.apperance
"補完関連
source ~/.vim/config/.vimrc.completion
"Tags関連
source ~/.vim/config/.vimrc.tags
"検索関連
source ~/.vim/config/.vimrc.search
"移動関連
source ~/.vim/config/.vimrc.moving
"Color関連
source ~/.vim/config/.vimrc.colors
"編集関連
source ~/.vim/config/.vimrc.editing
"エンコーディング関連
source ~/.vim/config/.vimrc.encoding
"その他
source ~/.vim/config/.vimrc.misc
"プラグインに依存するアレ
source ~/.vim/config/.vimrc.plugins_setting
"tab周りの設定
source ~/.vim/config/.vimrc.tab
